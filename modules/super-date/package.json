  },
  "author": "Adarsh Pastakia <adarshpastakia@outlook.com>",
  "license": "MIT",
  "main": "dist/index.js",
  "directories": {
    "test": "__tests__",
    "lib": "src"
  },
  "dependencies": {
    "@ant-design/icons": "^4.0.6",
    "antd": "^4.1.4",
    "date-fns": "^2.12.0",
    "i18next": "^19.4.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-i18next": "^11.3.5"
  },
  "devDependencies": {
    "@storybook/addon-knobs": "^5.3.18",
    "@types/storybook__addon-knobs": "^5.2.1"
  },
  "scripts": {
    "build:lib": "tsc -p ./tsconfig.build.json",
