{
  "name": "@ant-extensions/super-date",
  "version": "0.0.3",
  "publishConfig": {
    "access": "public"
  },
  "description": "Ant React Extended - Super date selector",
  "keywords": [
    "antd.4",
    "superdate",
    "relative date picker"
  ],
  "repository": {
